{
  "name": "developmentenv",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "serve": "vite serve",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
		"prepare": "husky install",
		"type-check": "vue-tsc --noEmit && vue-tsc --noEmit -p tests/tsconfig.json"
  },
  "dependencies": {
    "vue": "^3.5.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/ui": "^2.1.3",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.13.0",
    "eslint-plugin-editorconfig": "^4.0.3",
    "eslint-plugin-vue": "^9.29.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "sass": "^1.80.3",
    "sass-loader": "^16.0.2",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.3",
    "vue-tsc": "^2.1.6"
  }
}
